<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
         	 android:layout_width="fill_parent"
		     android:layout_height="wrap_content"
		     xmlns:tools="http://schemas.android.com/tools">
<ScrollView
            android:id="@+id/scrollView1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            
        
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"        
        android:orientation="vertical" >
        
        <ImageButton
                android:id="@+id/close"
                android:layout_width="match_parent"
                android:layout_height="120dp"                            
                android:background="#FFFFFF"
                android:contentDescription="@string/desc"
                android:src="@drawable/ficha_banner" />
        <TextView
                        android:id="@+id/titulo_accion_usuario"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"                        
                        android:background="#FFFFFF"
                        android:layout_gravity="center"
                        android:text="@string/datos_generales"
                        android:textColor="#011254"
                        android:textSize="21sp"
                        android:textStyle="bold"
                                                
                        />

        
        
        <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"        
        android:orientation="horizontal" 
        >
        	<TextView
                        android:id="@+id/tvcorrelativo"
                        android:layout_width="175dp"
                        android:layout_height="wrap_content"                        
                        android:background="#FFFFFF"
                        android:gravity="center"
                        android:text="@string/correlativo_miembro_familia"
                        android:textColor="#011254"
                        android:textStyle="normal"
                        style="@style/alturapreguntas"                        
                        />

        	

        	    
        	

        	<EditText
        	    android:id="@+id/etcorrelativo"
        	    android:layout_width="100dp"
        	    android:layout_height="wrap_content"        	            	    
        	    android:maxLength="2"
        	    android:singleLine="true"
        	    android:gravity="center"
        	    android:inputType="number" 
        	    style="@style/alturapreguntas">
        	    
        	</EditText>
        </LinearLayout>
        
        <TextView
                        android:id="@+id/tvrotulonombreapellidos"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"                        
                        android:background="#FFFFFF"
                        android:layout_gravity="center"
                        android:text="@string/nombresyapellidos"
                        android:textColor="#011254"
                        android:textStyle="normal" 
                        style="@style/alturapreguntas"
                        />
        <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_gravity="center"        
        android:orientation="horizontal" >
        	<TextView
                        android:id="@+id/tvprimernombre"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"                        
                        android:background="#FFFFFF"
                        android:layout_gravity="center"
                        android:text="@string/primer_nombre"
                        android:textColor="#011254"
                        android:textStyle="normal" 
                        style="@style/alturapreguntas"
                        />

        	<EditText
        	    android:id="@+id/etprimernombre"
        	    android:layout_width="0dp"
        	    android:layout_height="wrap_content"
        	    android:layout_weight="1"        	    
        	    android:singleLine="true"
        	    android:maxLength="100"
        	    android:width="200dp"
        	    style="@style/alturapreguntas"
        	    android:inputType="textCapCharacters"
        	    android:digits="ABCDEFGHIJKLMNÑOPQRSTUVWXYZ ">

        	    
        	</EditText>
			<TextView
                        android:id="@+id/tvsegundonombre"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"                        
                        android:background="#FFFFFF"
                        android:layout_gravity="center"
                        android:text="@string/segundo_nombre"
                        android:textColor="#011254"
                        android:textStyle="normal"
                        style="@style/alturapreguntas" 
                        />

        	<EditText
        	    android:id="@+id/etsegundonombre"
        	    android:layout_width="0dp"
        	    android:layout_height="wrap_content"
        	    android:layout_weight="1"
        	    android:ems="10"
        	    android:singleLine="true"
        	    android:maxLength="100"
        	    android:width="200dp"
        	    android:inputType="textCapCharacters"
        	    android:digits="ABCDEFGHIJKLMNÑOPQRSTUVWXYZ " 
        	    style="@style/alturapreguntas">

        	    
        	</EditText>
        	<TextView
                        android:id="@+id/tvtercernombre"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"                        
                        android:background="#FFFFFF"
                        android:layout_gravity="center"
                        android:text="@string/tercer_nombre"
                        android:textColor="#011254"
                        android:textStyle="normal"
                        style="@style/alturapreguntas" 
                        />

        	<EditText
        	    android:id="@+id/ettercernombre"
        	    android:layout_width="0dp"
        	    android:layout_height="wrap_content"
        	    android:layout_weight="1"
        	    android:ems="10"
        	    android:singleLine="true"
        	    android:maxLength="100"
        	    android:width="200dp"
        	    android:inputType="textCapCharacters"
        	    android:digits="ABCDEFGHIJKLMNÑOPQRSTUVWXYZ " 
        	    style="@style/alturapreguntas">

        	    
        	</EditText>
        </LinearLayout>
        <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"        
        android:orientation="horizontal" 
        android:layout_gravity="center">
        	<TextView
                        android:id="@+id/tvprimerapellido"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"                        
                        android:background="#FFFFFF"
                        android:layout_gravity="center"
                        android:text="@string/primer_apellido"
                        android:textColor="#011254"
                        android:textStyle="normal"
                        style="@style/alturapreguntas" 
                        />
        	
        	<EditText
        	    android:id="@+id/etprimerapellido"
        	    android:layout_width="0dp"
        	    android:layout_height="wrap_content"
        	    android:layout_weight="1"
        	    android:ems="10"
        	    android:singleLine="true"
        	    android:maxLength="100"
        	    android:width="200dp"
        	    android:inputType="textCapCharacters"
        	    android:digits="ABCDEFGHIJKLMNÑOPQRSTUVWXYZ " 
        	    style="@style/alturapreguntas">

        	    
        	</EditText>
			<TextView
                        android:id="@+id/tvsegundoapellido"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"                        
                        android:background="#FFFFFF"
                        android:layout_gravity="center"
                        android:text="@string/segundo_apellido"
                        android:textColor="#011254"
                        android:textStyle="normal"
                        style="@style/alturapreguntas" 
                        />

        	<EditText
        	    android:id="@+id/etsegundoapellido"
        	    android:layout_width="0dp"
        	    android:layout_height="wrap_content"
        	    android:layout_weight="1"
        	    android:ems="10"
        	    android:singleLine="true"
        	    android:maxLength="100"
        	    android:width="200dp"
        	    android:inputType="textCapCharacters"
        	    android:digits="ABCDEFGHIJKLMNÑOPQRSTUVWXYZ " 
        	    style="@style/alturapreguntas">

        	    
        	</EditText>
        	<TextView
                        android:id="@+id/tvapellidocasada"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"                        
                        android:background="#FFFFFF"
                        android:layout_gravity="center"
                        android:text="@string/apellido_casada"
                        android:textColor="#011254"
                        android:textStyle="normal"
                        style="@style/alturapreguntas" 
                        />

        	<EditText
        	    android:id="@+id/etapellidocasada"
        	    android:layout_width="0dp"
        	    android:layout_height="wrap_content"
        	    android:layout_weight="1"        	    
        	    android:singleLine="true"
        	    android:maxLength="100"
        	    android:width="200dp"
        	    android:inputType="textCapCharacters"
        	    android:digits="ABCDEFGHIJKLMNÑOPQRSTUVWXYZ "
        	    style="@style/alturapreguntas" 
        	    >        	    
        	</EditText>
        </LinearLayout>
        <LinearLayout
        	    android:layout_width="900dp"
        	    android:layout_height="match_parent"
        	    android:layout_gravity="center" 
        	    android:orientation="horizontal">

        	<TextView
        	    android:id="@+id/tvtienedocumentoidentidad"
        	    android:layout_width="225dp"
        	    android:layout_height="wrap_content"
        	    android:layout_gravity="center"
        	    android:background="#FFFFFF"
        	    android:text="@string/tienedocumentodeidentidad"
        	    android:textColor="#011254"
        	    android:textStyle="normal" 
        	    style="@style/alturapreguntas"/>
        	<Spinner
                    android:id="@+id/SpnTieneDocumento"
                    android:layout_width="225dp"
                    android:layout_height="45dp"                    
                    android:background="#FFFFFF" 
                    android:width="200dp"
                    android:height="80dp"                                       
            />
		
		
        	<TextView
                        android:id="@+id/tvnumerodui"
                        android:layout_width="225dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"                                                
                        android:background="#FFFFFF"                                               
                        android:text="@string/numerodui"
                        android:textColor="#011254"
                        android:textStyle="normal"
                        style="@style/alturapreguntas" 
                        />
        	
        	<EditText
        	    android:id="@+id/etnumerodui"
        	    android:layout_width="225dp"
        	    android:layout_height="wrap_content"
        	    android:layout_weight="1"        	    
        	    android:singleLine="true"
        	    android:maxLength="9"
        	    android:width="130dp"
        	    android:inputType="number" 
        	    style="@style/alturapreguntas">
            </EditText>
        </LinearLayout>
		<LinearLayout
		    android:layout_width="900dp"
        	    android:layout_height="match_parent"
        	    android:layout_gravity="center" 
        	    android:orientation="horizontal">	
        	
        	    <TextView
        	        android:id="@+id/tvnacionalidad"        	        
        	        android:layout_width="225dp"
                   	android:layout_height="45dp"
                   	android:textColor="#011254"                                                  
                    android:text="@string/nacionalidad"
                    style="@style/alturapreguntas"/>
        	    
                <Spinner 
                   	android:id="@+id/SpnNacionalidad"
                    android:layout_width="225dp"
	           		android:layout_height="45dp"
	           		android:background="#FFFFFF" 				                     
	                android:width="200dp" 
	                style="@style/alturapreguntas"
				/>
                <TextView
                    android:layout_width="225dp"
                    android:layout_height="45dp"
                    android:text="@string/sexo"
                    android:textColor="#011254"
                    
                    style="@style/alturapreguntas"
                    
                     />
                <Spinner
                    android:id="@+id/SpnSexo"
                    android:layout_width="225dp"
                    android:layout_height="45dp"                    
                    android:background="#FFFFFF" 
                    android:width="200dp"
                    style="@style/alturapreguntas"/>
        	</LinearLayout>
        	<LinearLayout 
        	    android:layout_width="900dp"
        	    android:layout_height="match_parent"
        	    android:layout_gravity="center" 
        	    android:orientation="horizontal">
        	    <TextView
                     android:layout_width="225dp"
                     android:layout_height="40dp"
                     android:text="@string/fecha_nacimiento"
                     android:textColor="#011254"
                     style="@style/alturapreguntas"
                     /> 
                 <EditText
                     android:id="@+id/txt_fecha_nacimiento"
                     android:layout_width="177dp"
                     android:layout_height="40dp"
                     style="@style/alturapreguntas"/>
                  <ImageButton
                  android:id="@+id/imageFechaNac"
                  android:layout_width="48dp"
                  android:layout_height="wrap_content"
                  android:src="@drawable/calendar"
                  android:background="#FFFFFF"
                  style="@style/alturapreguntas"
                    />
                  <TextView
                     android:layout_width="225dp"
                     android:layout_height="40dp"
                     android:text="@string/edad"                     
                     android:textColor="#011254"
                     style="@style/alturapreguntas"
                      />
                 <TextView
                     android:id="@+id/tvedadcalculada"
                     android:layout_width="225dp"
                     android:layout_height="40dp"
                     android:text="EDAD CALCULADA"                     
                     android:textColor="#011254" 
                     style="@style/alturapreguntas"
                      />       
        	</LinearLayout>
		<LinearLayout
		    android:layout_width="900dp"
        	    android:layout_height="match_parent"
        	    android:layout_gravity="center" 
        	    android:orientation="horizontal">
                 
        	    <TextView 
        	        android:id="@+id/tvparentesto"
        	        android:background="#FFFFFF" 
                    android:layout_width="260dp"
               		android:layout_height="45dp"                                                  
                    android:text="@string/parentesco_con_jefe_familia"
                    android:textColor="#011254"
                    style="@style/alturapreguntas"/>
                <Spinner
                    android:id="@+id/Spnparentesto" 
                    android:background="#FFFFFF"                      
                    android:layout_weight="0.3" 
                    android:layout_width="190dp"
                	android:layout_height="45dp"
                	style="@style/alturapreguntas"/>
        	</LinearLayout>
        	<LinearLayout
		    	android:layout_width="900dp"
        	    android:layout_height="match_parent"
        	    android:layout_gravity="center" 
        	    android:orientation="horizontal">
		        	<TextView
		                        android:id="@+id/tvnombremadre"
		                        android:background="#FFFFFF"
		                        android:layout_width="225dp"
		                        android:layout_height="45dp"                        
		                        android:text="@string/nombre_madre"
		                        android:textColor="#011254"
		                        style="@style/alturapreguntas"
		                        />
		
		        	<EditText
		        	    android:id="@+id/etnombremadre"
		        	    android:layout_width="225dp"
		        	    android:layout_height="45dp"
		        	    android:layout_weight="1"        	    
		        	    android:singleLine="true"
		        	    android:maxLength="100"        	    
		        	    android:inputType="textCapCharacters"
		        	    android:digits="ABCDEFGHIJKLMNÑOPQRSTUVWXYZ "
		        	    style="@style/alturapreguntas">
		
		        	    
		        	</EditText>
        	</LinearLayout>
         	
		<LinearLayout
		    android:layout_width="900dp"
        	    android:layout_height="match_parent"
        	    android:layout_gravity="center" 
        	    android:orientation="horizontal"> 
			<TextView
                        android:id="@+id/tvnombrepadre"
                        android:background="#FFFFFF"
                        android:layout_width="225dp"
                        android:layout_height="45dp"                                                
                        android:text="@string/nombre_padre"
                        android:textColor="#011254"
                        style="@style/alturapreguntas" 
                        />

        	<EditText
        	    android:id="@+id/etnombrepadre"
        	    android:layout_width="225dp"
        	    android:layout_height="wrap_content"
        	    android:layout_weight="1"
        	    android:ems="10"
        	    android:singleLine="true"
        	    android:maxLength="100"
        	    
        	    android:inputType="textCapCharacters"
        	    android:digits="ABCDEFGHIJKLMNÑOPQRSTUVWXYZ " 
        	    style="@style/alturapreguntas">

        	    
        	</EditText>
        	</LinearLayout>
        	<LinearLayout
		    android:layout_width="900dp"
        	    android:layout_height="match_parent"
        	    android:layout_gravity="center" 
        	    android:orientation="horizontal">
        	<TextView
                        android:id="@+id/tvnombreresponsable"
                        android:background="#FFFFFF"
                        android:layout_width="225dp"
                        android:layout_height="45dp"                                                                        
                        android:text="@string/nombre_responsable"
                        android:textColor="#011254"
                        style="@style/alturapreguntas" 
                        />

        	<EditText
        	    android:id="@+id/etnombre_responsable"
        	    android:layout_width="225dp"
        	    android:layout_height="wrap_content"
        	    android:layout_weight="1"
        	    android:ems="10"
        	    android:singleLine="true"
        	    android:maxLength="100"
        	    
        	    android:inputType="textCapCharacters"
        	    android:digits="ABCDEFGHIJKLMNÑOPQRSTUVWXYZ " 
        	    style="@style/alturapreguntas">

        	    
        	</EditText>
        </LinearLayout>
        <LinearLayout 
            android:layout_width="900dp"
	        android:layout_height="match_parent"
	        android:layout_gravity="center"        
	        android:orientation="horizontal">
        	
            					<TextView 
                					 android:id="@+id/tvestadofamiliar"
                					 android:background="#FFFFFF" 
		                             android:layout_width="225dp"
		                     		 android:layout_height="45dp"                                                  
		                             android:text="@string/estado_familiar"
		                             android:textColor="#011254"
		                             style="@style/alturapreguntas"
		                             />
		                           <Spinner android:background="#FFFFFF" 
		                              
		                             android:id="@+id/SpnEstadoFamiliar" 		                             
		                             android:layout_width="225dp"
		                    		 android:layout_height="45dp"
		                    		 style="@style/alturapreguntas"/>
		                           
			<TextView android:background="#FFFFFF" 
			    android:id="@+id/tvaseguradobeneficiario"
		                             android:layout_margin="1dip" 
									 android:layout_width="225dp"
		                     		 android:layout_height="45dp"                                                  
		                             android:text="@string/aseguradobeneficiario"
		                             android:textColor="#011254"
		                             
		                             style="@style/alturapreguntas"/>
			
		                           <Spinner android:background="#FFFFFF" 
		                              
		                             android:id="@+id/SpnAseguradoBeneficiario" 
		                              
		                             android:layout_width="225dp"
		                    		 android:layout_height="45dp"
		                    		 style="@style/alturapreguntas"/>
	                          </LinearLayout>
              		          <LinearLayout
							        android:layout_width="match_parent"
							        android:layout_height="match_parent"
							        android:gravity="center"        
							        android:orientation="horizontal" 
							        >
							        	<TextView
							                        android:id="@+id/tvinstitucioncotizantebeneficiario"
							                        android:layout_width="325dp"
							                        android:layout_height="wrap_content"                        
							                        android:background="#FFFFFF"
							                        android:gravity="center"
							                        android:text="@string/institucion_cotizante_beneficiario"
							                        android:textColor="#011254"
							                        android:textStyle="normal"
							                        style="@style/alturapreguntas"                        
							                        />                 
           								
				                     		 
				                             
            
		                           		<Spinner android:background="#FFFFFF" 		                             
		                             		 android:id="@+id/SpnInstitucion_cotizante_beneficiario" 		                              
				                             android:layout_width="225dp"
				                    		 android:layout_height="45dp"
				                    		 style="@style/alturapreguntas"
				                    		 
				                    		 /> 
            	
        						</LinearLayout>
	         
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"                         
            android:orientation="horizontal"
            android:gravity="center">

            <ImageButton
                android:id="@+id/imgbuttonnewmiembro"
                android:layout_width="120dp"
                android:layout_height="80dp"                
                android:background="#FFFFFF"
                android:layout_gravity="center"
                android:onClick="add_miembro_familia"
                android:src="@drawable/save" />            
        </LinearLayout>
    </LinearLayout>

            
</ScrollView>    
    
</FrameLayout>
