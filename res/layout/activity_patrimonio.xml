<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content" >
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:context=".Ficha_familia_activity_01" >
        <RelativeLayout
            android:id="@+id/relativeLayout1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="2dp" >
            <ImageButton
                android:id="@+id/close"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_alignParentLeft="true"
                android:layout_marginTop="10dp"
                android:background="#FFFFFF"
                android:contentDescription="@string/desc"
                android:src="@drawable/ficha_banner" />
        </RelativeLayout>
        <ScrollView
            android:id="@+id/scrollView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <RelativeLayout
                android:layout_width="1014dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="125dp"
                android:layout_marginTop="160dp" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginTop="20dp" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="1dip"
                        android:layout_weight="1.04"
                        android:background="#FFFFFF"
                        android:gravity="center_horizontal"
                        android:text="@string/patrimonio_vivienda"
                        android:textColor="#011254"
                        android:textSize="21sp"
                        android:textStyle="bold" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="70dp" >

                    <TextView
                        android:layout_width="189dp"
                        android:layout_height="40dp"
                        android:background="#FFFFFF"
                        android:text="@string/num_habitaciones"
                        android:textColor="#011254" />

                    <TextView
                        android:layout_width="248dp"
                        android:layout_height="75dp"
                        android:layout_marginLeft="160dp"
                        android:background="#FFFFFF"
                        android:text="@string/otros_espacios"
                        android:textColor="#011254" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:layout_marginLeft="135dp"
                        android:background="#FFFFFF"
                        android:text="@string/bienes_familia"
                        android:textColor="#011254" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="60dp" >

                    <EditText
                        android:id="@+id/NumHabitaciones"
                        android:layout_width="69dp"
                        android:layout_height="40dp"
                        android:layout_marginLeft="80dp"
                        android:layout_marginTop="20dp"
                        android:ems="10"
                    	android:inputType="number"
                   		android:maxLength="2" />

                    <EditText
                        android:id="@+id/NoHabitacionesDormitorios"
                        android:layout_width="69dp"
                        android:layout_height="40dp"
                        android:layout_marginLeft="290dp"
                        android:layout_marginTop="40dp"
                        android:ems="10"
                        android:inputType="number"
                        android:maxLength="2" >
                    </EditText>

                    
                </LinearLayout>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="55dp" >
                <CheckBox
                        android:id="@+id/CheckcultivoAgricolaPropio"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="748dp"
                        android:layout_marginTop="30dp" 
                        android:text="@string/cultivo_agricola_propio"
                        android:onClick="click_CultivoAgricolaPropio"
                        android:textColor="#004B0F"
                        android:textSize="14sp" />
                </LinearLayout>
                
				<LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="70dp" >

                    <CheckBox
                        android:id="@+id/Checkavescorral"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="748dp"
                        android:layout_marginTop="40dp"
                        android:text="@string/aves_corral"
                        android:onClick="click_AvesCorral"
                        android:textColor="#004B0F"
                        android:textSize="14sp" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="95dp" >

                    <CheckBox
                        android:id="@+id/Checkganado_vacuno"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="748dp"
                        android:layout_marginTop="40dp" 
                        android:text="@string/ganado_vacuno"
                        android:onClick="clic_GanadoVacuno"
                        android:textColor="#004B0F"
                        android:textSize="14sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="120dp" >

                    <CheckBox
                        android:id="@+id/Checkganado_porcino"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="748dp"
                        android:layout_marginTop="40dp"
                        android:text="@string/ganado_porcino"
                        android:onClick="click_GanadoPorcino"
                        android:textColor="#004B0F"
                        android:textSize="14sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="120dp" >

                    <CheckBox
                        android:id="@+id/Checknegocio_propio"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="748dp"
                        android:layout_marginTop="65dp" 
                        android:text="@string/negocio_propio"
                        android:onClick="click_NegocioPropio"
                        android:textColor="#004B0F"
                        android:textSize="14sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="120dp" >

                    <CheckBox
                        android:id="@+id/Checkninguno1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="748dp"
                        android:layout_marginTop="90dp" 
                        android:text="@string/ninguno"
                        android:onClick="click_ninguno1"
                        android:textColor="#004B0F"
                        android:textSize="14sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="160dp" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:background="#FFFFFF"
                        android:text="@string/bienes_hogar"
                        android:textColor="#011254" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="100dp" >

                    <CheckBox
                        android:id="@+id/Checkradio"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="75dp" 
                        android:text="@string/radio"
                        android:onClick="click_Radio"
                        android:textColor="#004B0F"
                        android:textSize="14sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="125dp" >

                    <CheckBox
                        android:id="@+id/Checkequipo_sonido"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="75dp" 
                        android:text="@string/equipo_sonido"
                        android:onClick="click_EquipoSonido"
                        android:textColor="#004B0F"
                        android:textSize="14sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="150dp" >

                    <CheckBox
                        android:id="@+id/Checktelevisor"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="75dp" 
                        android:text="@string/televisor"
                        android:onClick="click_Televisor"
                        android:textColor="#004B0F"
                        android:textSize="14sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="175dp" >

                    <CheckBox
                        android:id="@+id/Checkrefrigeradora"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="75dp" 
                        android:text="@string/refrigeradora"
                        android:onClick="click_Refrigeradora"
                        android:textColor="#004B0F"
                        android:textSize="14sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="200dp" >

                    <CheckBox
                        android:id="@+id/Checklavadora"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="75dp" 
                        android:text="@string/lavadora"
                        android:onClick="click_lavadora"
                        android:textColor="#004B0F"
                        android:textSize="14sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="225dp" >

                    <CheckBox
                        android:id="@+id/Checkcomputadora"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="75dp" 
                        android:text="@string/computadora"
                        android:onClick="click_Computadora"
                        android:textColor="#004B0F"
                        android:textSize="14sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="250dp" >
                    <CheckBox
                        android:id="@+id/Checkninguno2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="75dp" 
                        android:text="@string/ninguno"
                        android:onClick="click_ninguno2"
                        android:textColor="#004B0F"
                        android:textSize="14sp"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="160dp" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:layout_marginLeft="350dp"
                        android:background="#FFFFFF"
                        android:text="@string/vehiculo"
                        android:textColor="#011254" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="150dp" >

                    <Spinner
                        android:id="@+id/SpnVehiculoHogar"
                        android:layout_width="100dp"
                        android:layout_height="40dp"
                        android:layout_marginLeft="555dp" />
                </LinearLayout>
            </RelativeLayout>

        </ScrollView>
       <LinearLayout 
				   	android:layout_width="80dp"
           			android:layout_height="90dp"
           			android:layout_marginTop="450dp"
           			android:layout_marginLeft="550dp"
           			>

           <ImageButton
               android:id="@+id/button1"
               android:layout_width="80dp"
               android:layout_height="90dp"
               android:background="#FFFFFF"
               android:onClick="click_patrimonio"
               android:src="@drawable/save" />
           
        </LinearLayout>

       

</RelativeLayout>
</ScrollView>